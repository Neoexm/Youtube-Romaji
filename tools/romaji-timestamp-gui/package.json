{
  "name": "romaji-timestamp-gui",
  "version": "1.0.0",
  "description": "Desktop GUI tool to timestamp Genius romaji to YouTube and push to DB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "kuromoji": "^0.1.2",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.romaji.timestamp-gui",
    "productName": "Romaji Timestamp GUI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!output/**/*",
      "!secrets.local.json"
    ]
  }
}